:alias px v1
:alias py v2

: main
  px := 14
	py := 13
	i := N
	sprite px py 7
	px += 8
	i := O
	sprite px py 7
	px += 10
	i := R
	sprite px py 7
	px += 6
	i := O
	sprite px py 7
	px += 5
	i := M
	sprite px py 7
	
	loop again
	

: N
  0x82 0xC2 0xA2 0x92 0x8A 0x86 0x82
	
: O
  0x00 0x00 0x60 0x90 0x90 0x90 0x60
	
: R
	0xF0 0x88 0x88 0x88 0xD0 0xA0 0x90

: M
	0x00 0x00 0x88 0xD8 0xA8 0x88 0x88